{
  "name": "use-xhr",
  "version": "0.0.1",
  "description": "React hook for chunked file upload",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": "mortzmortz/use-xhr",
  "homepage": "http://github.com/mortzmortz/use-xhr",
  "author": "Gerion Herbst",
  "license": "MIT",
  "keywords": [
    "react",
    "hooks",
    "file-upload",
    "chunk-upload"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "rollup -c -w",
    "test": "jest src --coverage --colors --passWithNoTests",
    "test:watch": "jest src --watch",
    "test:all": "yarn lint && yarn test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "prepack": "yarn clean && yarn build"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "watchPathIgnorePatterns": [
      "dist"
    ],
    "collectCoverageFrom": [
      "src/**.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react-hooks": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "1.x",
    "@typescript-eslint/parser": "1.x",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.x",
    "coveralls": "^3.0.6",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.20.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "dependencies": {
    "uuid": "^3.3.3"
  }
}
